@model businessLogic.interfaces.IListModelCollection


@{
    ViewBag.Title = "viewData";
    Layout = "~/Views/shared/_Layout.cshtml";
}

<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand" href="#">Paging & Sorting</a>
    <div class="float-right">
        @Html.ActionLink("Pull Data Item #1", "pagedList", "dbItemTypeOne", null, new { @class = "btn btn-outline-success my-2 my-sm-0" })
        @Html.ActionLink("Pull Data Item #2", "pagedList", "dbItemTypeTwo", null, new { @class = "btn btn-outline-success my-2 my-sm-0" })
    </div>
</nav>
<div class="container-fluid">
    <div id="filterDataForm">
        <br />
        @Html.Partial($"~/Views/genericCrudList/_pagedListFilterFormData.cshtml", Model.filterFormModel)
        <br />
    </div>
    <div id="viewSectionPartial">
        @Html.Partial($"~/Views/genericCrudList/_pagedListPartial.cshtml", Model)
    </div>   
</div>
